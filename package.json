{
  "name": "vscode-onboarding",
  "displayName": "vscode-onboarding",
  "description": "Sophio's SonarLint Onboarding",
  "publisher": "sophio",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sophio-japharidze-sonarsource/vscode-onboarding.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-onboarding.heyThere",
    "onCommand:vscode-onboarding.time",
    "onView:greetings"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-onboarding.heyThere",
        "title": "Hey there"
      },
      {
        "command": "vscode-onboarding.time",
        "title": "Time"
      }
    ],
    "keybindings": {
      "command": "vscode-onboarding.time",
      "key": "ctrl+T",
      "mac": "cmd+T",
      "when": "isMac || isLinux || isWindows"
    },
    "views": {
      "explorer": [
        {
          "id": "greetings",
          "name": "Greetings",
          "when": "isMac || isLinux || isWindows",
          "icon": "./calm.png",
          "contextualTitle": "Get Greeted"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && webpack --mode production",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "moment": "^2.29.3",
    "ts-loader": "^9.3.0",
    "vsce": "^2.7.0"
  }
}
